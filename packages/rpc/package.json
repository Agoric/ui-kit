{
  "name": "@agoric/rpc",
  "description": "RPC Client Library for Agoric Dapps",
  "license": "Apache-2.0",
  "version": "0.9.0",
  "type": "module",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
