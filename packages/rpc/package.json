    "@vitest/coverage-c8": "^0.25.3",
    "axios-mock-adapter": "^1.22.0",
    "eslint": "^8.22.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-require-extensions": "^0.1.3",
    "happy-dom": "^9.20.3",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.7.1",
    "typescript": "^5.1.3",
    "vitest": "^0.34.1"
