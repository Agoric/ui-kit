{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "checkJs": false,
    "strict": false,
    "downlevelIteration": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noEmit": true
  },
  "include": [
    ".eslintrc.cjs"
  ]
}
